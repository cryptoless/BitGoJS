{
  "name": "@bitgo/utxo-lib",
  "version": "2.0.0",
  "description": "Client-side Bitcoin JavaScript library",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "engines": {
    "node": ">=10.22.0 <17",
    "npm": ">=3.10.10"
  },
  "keywords": [
    "bitgo",
    "utxo",
    "bitcoinjs",
    "bitcoin",
    "zcash",
    "dash",
    "browserify",
    "javascript"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "coverage-report": "nyc report --reporter=lcov",
    "coverage-html": "nyc report --reporter=html",
    "coverage": "npm run build && nyc --check-coverage mocha",
    "test": "npm run coverage",
    "unit-test": "mocha",
    "lint": "eslint --quiet .",
    "fmt": "prettier --write '{src,test}/**/*.{ts,js}'"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "branches": 78,
    "functions": 90
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cryptoless/BitGoJS.git",
    "directory": "modules/utxo-lib"
  },
  "files": [
    "dist/src"
  ],
  "dependencies": {
    "@bitgo/blake2b": "^3.0.1",
    "bitcoin-ops": "^1.3.0",
    "bitcoinjs-lib": "git+https://github.com/cryptoless/bitcoinjs-lib.git#b870a6ec65610bded74d5d1e77c4038c49f96541",
    "bs58check": "^2.0.0",
    "typeforce": "^1.11.3",
    "varuint-bitcoin": "^1.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "axios": "^0.21.1",
    "debug": "^3.1.0",
    "fs-extra": "^9.1.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  },
  "license": "MIT"
}